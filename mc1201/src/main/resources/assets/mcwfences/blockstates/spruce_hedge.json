{
  "multipart": [
    {
      "apply": {
        "model": "mcwfences:block/hedges/spruce_wall_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "mcwfences:block/hedges/spruce_wall_side_tall",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "mcwfences:block/hedges/spruce_wall_side_tall",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "mcwfences:block/hedges/spruce_wall_side_tall",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "mcwfences:block/hedges/spruce_wall_side_tall",
        "y": 270,
        "uvlock": false
      }
    }
  ]
}